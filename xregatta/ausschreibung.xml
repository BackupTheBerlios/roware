			<item id="1500" einheit="m">1500</item>
			<item id="2000" einheit="m">2000</item>
		</strecken>

		<!-- Klassifizierung. Es werden Gruppen aus kosten, strecken, pokal, lgw, "renngemeinschaft
		erlaubt", Nachmeldegebühr und Reuegeld geschnürt. Sozusagen die verwendeten kombinationen aus
		der Potenzmenge. Attribute per default auf lgw=false, pokal=false, rgm=true, nakoid = koid, 
		rekoid=leer -->
		<!-- id = id, koid= kostenid, nakoid = kostenid für nachmeldung, rekoid = kostenid für 
		reuegeld, stid=streckenid -->
		<klassen>
			<!-- normale Rennen 1er, 2er, 4er, 8er-->
			<item id="1" koid="1" nakoid="1N" rekoid="R1" stid="1000"/>
			<item id="2" koid="2" nakoid="2N" rekoid="R1" stid="1000"/>
			<item id="4" koid="4" nakoid="4N" rekoid="R2" stid="1000"/>
			<item id="8" koid="8" nakoid="8N" rekoid="R2" stid="1000"/>

			<!-- leichtgewichts Rennen 1er, 2er, 4er, 8er -->
			<item id="L1" koid="1" stid="1000" rekoid="R1" lgw="true"/>
			<item id="L2" koid="2" stid="1000" rekoid="R1" lgw="true"/>
			<item id="L4" koid="4" stid="1000" rekoid="R2" lgw="true"/>
			<item id="L8" koid="8" stid="1000" rekoid="R2" lgw="true"/>

			<!-- Pokal Rennen 1er, 2er, 4er, 8er -->
			<item id="P2" koid="2" stid="1000" rekoid="R1" pokal="true"/>
			<item id="P4" koid="4" stid="1000" rekoid="R1" pokal="true"/>
			<item id="P8" koid="8" stid="1000" rekoid="R2" pokal="true"/>

			<!-- Kinder Rennen 1er, 2er, 4er, 8er -->
			<item id="J1" koid="J1" stid="1000" rekoid="R1" rgm="false"/>
			<item id="J2" koid="J2" stid="1000" rekoid="R1" rgm="false"/>
			<item id="J4" koid="J4" stid="1000" rekoid="R2" rgm="false"/>
		</klassen>

		<!-- liste aller verwendeten Datumsangaben im Format
			YYYYMMDD oder YYYYMMDDTHHMM -->
		<datumsliste>
			<item id="regattatag1">20031003</item>
			<item id="regattatag2">20031004T0900</item>
			<item id="auslosung">20030924T1900</item>
			<item id="meldeschluss">20030924T1800</item>
		</datumsliste>

		<!-- liste aller rennen. id ist die nr des rennens -->
		<!-- generell: Die genaue Rennbezeichnung ergibt sich aus <bez/>, dem klassenattribut lgw und
		der Leistungsklasse -->
		<rennliste>
			<rennen id="1" klid="2">
				<!-- klid = id der verwendeten klasse, in diesem fall "normale rennen 2x" -->
				<bez>SM A 2x</bez>
				<!-- Kombination der Leistunsgklassen -->
				<leistungsklassen>
					<item>I</item>
				</leistungsklassen>
				<bemerkung/>
			</rennen>
			<rennen id="2" klid="2" koid="8">
				<!--koid = kostenid -> rennen ist so teuer wie ein 8er -->
				<bez>SF A 2x</bez>
				<leistungsklassen>
					<item>I</item>
					<item>III</item>
				</leistungsklassen>
				<bemerkung/>
			</rennen>
		</rennliste>

